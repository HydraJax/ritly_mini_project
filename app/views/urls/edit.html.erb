
<body>
	<link href='http://fonts.googleapis.com/css?family=Open+Sans+Condensed:300' rel='stylesheet' type='text/css'>

<h1>Edit Your Edit</h1>
<%= form_for @url do |f| %>

  <%= label_tag(:link, "Link:") %>
  <%= f.text_field :link %></br>

  <%= label_tag(:random_string, "Code") %>
  <%= f.text_area :random_string %></br>

  <%= f.submit "Update" %>
<% end %>

</body>